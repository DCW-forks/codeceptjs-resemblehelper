{
  "name": "codeceptjs-resemblehelper",
  "version": "1.9.3",
  "description": "Resemble Js helper for CodeceptJS, with Support for Playwright, Webdriver, TestCafe, Puppeteer & Appium",
  "scripts": {
    "lint": "eslint index.js tools configs tests",
    "lint-fix": "eslint index.js tools configs tests --fix",
    "config-prepareBaseImage:undefined": "codeceptjs run --config=./configs/prepareBaseImage-undefined.conf.js  --debug",
    "config-prepareBaseImage:true": "codeceptjs run --config=./configs/prepareBaseImage-true.conf.js  --debug",
    "config-prepareBaseImage:false": "codeceptjs run --config=./configs/prepareBaseImage-false.conf.js  --debug"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Percona-Lab/codeceptjs-resemblehelper.git"
  },
  "dependencies": {
    "assert": "^1.5.0",
    "canvas": "^2.6.1",
    "mz": "^2.7.0",
    "resemblejs": "^3.2.4",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "aws-sdk": "^2.662.0",
    "image-size": "^0.8.3"
  },
  "devDependencies": {
    "@wdio/cli": "^6.12.0",
    "allure-commandline": "^2.13.0",
    "codeceptjs": "^3.0.4",
    "codeceptjs-chai": "^2.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^6.2.2",
    "mochawesome": "^4.1.0",
    "webdriverio": "^6.12.0"
  },
  "keywords": [
    "codeceptJS",
    "codeceptjs",
    "resemblejs",
    "codeceptjs-resemble"
  ],
  "author": "Puneet Kala <puneet.kala@percona.com>",
  "license": "MIT"
}
