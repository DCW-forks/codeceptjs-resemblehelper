{
    "extends": "airbnb-base",
    "env": {
      "node": true
    },
    "rules": {
      "no-underscore-dangle": 0,
      "no-undef": 0,
      "prefer-destructuring": 0,
      "no-param-reassign": 0,
      "max-len": 0,
      "no-console": 0,
      "class-methods-use-this": 0,
      "no-multi-assign": 0,
      "no-bitwise": ["error", { "allow": ["|"] }],
      "prefer-const": 0,
      "dot-notation": 0,
      "object-shorthand": 0,
      "no-buffer-constructor": 0,
      "no-return-await":0
    }
  }